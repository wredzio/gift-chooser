    "mobx-react": "^5.2.6",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "2.0.4"
  },
  "devDependencies": {
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "parcel-bundler": "^1.9.7",
    "parcel-plugin-typed-css-modules": "^1.1.0",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
    "tslint-config-prettier": "^1.13.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.9.2"
  },
  "eslintConfig": {
    "extends": "react-app"
